{
    "_comment": "A custom .prettierrc file is here because Formik gets pretty intense.",
    "trailingComma": "none",
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 160,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "preserve",
    "jsxSingleQuote": false,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "avoid",
    "proseWrap": "never",
    "endOfLine": "lf",
    "embeddedLanguageFormatting": "auto",
    "singleAttributePerLine": true
}
